<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".withdrawScreen">

    <ImageView
        android:id="@+id/backgroundImage8"
        android:layout_width="0dp"
        android:layout_height="663dp"
        android:layout_marginTop="120dp"
        android:src="@drawable/cute_panda_paws_up_wall_panda_face_cartoon_icon_42750_498"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/bambooback" />

    <ImageView
        android:id="@+id/logo"
        android:layout_width="94dp"
        android:layout_height="95dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/newpandalogo"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/logoName"
        android:layout_width="68dp"
        android:layout_height="28dp"
        android:layout_marginTop="64dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.09"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/logo"
        app:srcCompat="@drawable/mutant_bamboo"
        />

    <Button
        android:id="@+id/twohundredbutton"
        android:layout_width="121dp"
        android:layout_height="51dp"
        android:layout_marginTop="376dp"
        android:background="#cc000000"
        android:text="$200"
        android:textColor="#FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.517"
        app:layout_constraintStart_toEndOf="@+id/sixtybutton"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="HardcodedText" />


    <Button
        android:id="@+id/onehundredbutton"
        android:layout_width="121dp"
        android:layout_height="51dp"
        android:layout_marginTop="308dp"
        android:background="#cc000000"
        android:text="$100"
        android:textColor="#FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.517"
        app:layout_constraintStart_toEndOf="@+id/fortybutton"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="HardcodedText" />

    <Button
        android:id="@+id/eightybutton"
        android:layout_width="121dp"
        android:layout_height="51dp"
        android:layout_marginTop="118dp"
        android:background="#cc000000"
        android:text="$80"
        android:textColor="#FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/twentybutton"
        app:layout_constraintTop_toTopOf="@+id/backgroundImage8"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/dateTxt4"
        android:layout_width="142dp"
        android:layout_height="31dp"
        android:layout_marginEnd="34dp"
        android:layout_marginBottom="59dp"
        android:text="Date"
        android:textColor="#111111"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <Button
        android:id="@+id/twentybutton"
        android:layout_width="121dp"
        android:layout_height="50dp"
        android:layout_marginTop="239dp"
        android:background="#cc000000"
        android:text="$20"
        android:textColor="#FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.189"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="HardcodedText" />

    <Button
        android:id="@+id/sixtybutton"
        android:layout_width="122dp"
        android:layout_height="49dp"
        android:layout_marginTop="379dp"
        android:background="#cc000000"
        android:text="$60"
        android:textColor="#FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.19"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/mainTxt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="591dp"
        android:text="Select an amount to withdraw."
        android:textColor="#111111"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/backgroundImage8"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/fortybutton"
        android:layout_width="123dp"
        android:layout_height="50dp"
        android:layout_marginTop="309dp"
        android:background="#cc000000"
        android:text="$40"
        android:textColor="#FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.19"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="HardcodedText" />

    <Button
        android:id="@+id/button8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="456dp"
        android:background="#3040E6"
        android:text=" Enter Different Amount "
        android:textColor="#FFFFFF"
        android:textSize="12sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.78"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/cancelBtn"
        android:layout_width="121dp"
        android:layout_height="48dp"
        android:layout_marginStart="55dp"
        android:layout_marginBottom="227dp"
        android:background="#B12525"
        android:text="cancel"
        android:textColor="#111111"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>